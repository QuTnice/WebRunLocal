<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <!--监听端口-->
    <add key="ListenerPort" value="9090"/>

    <!--是否开机自动启动-->
    <add key="AutoStart" value="true"/>

    <!--是否打印输入输出日志-->
    <add key="PramaterLoggerPrint" value="true"/>

    <!--是否创建桌面快捷方式-->
    <add key="DesktopLnk" value="false"/>
    <!--快捷方式名称-->
    <add key="QuickLnkName" value="插件管理工具"/>
  </appSettings>
  
</configuration>