<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <!--监听端口-->
    <add key="ListenerPort" value="8090"/>

    <!--是否开机自动启动-->
    <add key="AutoStart" value="false"/>

    <!--是否打印输入输出日志-->
    <add key="PramaterLoggerPrint" value="true"/>

    <!--是否创建桌面快捷方式-->
    <add key="DesktopLnk" value="false"/>

  </appSettings>
  
  
</configuration>